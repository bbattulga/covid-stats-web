<html>
  <head>
    <%- include('partials/head.ejs') %> <%- include('partials/scripts.ejs') %>
  </head>
  <body>
    <div>
      <header class="header p-3 mb-3">
        <div class="container-fluid">
          <div class="row">
            <div class="col">
              <div
                class="d-flex flex-row flex-column flex-md-row justify-content-between align-items-center"
              >
                <h1 class="header-title">
                  COVID-19 GLOBAL CASES AND STATISTICS
                </h1>
                <p>Developed by John Doe</p>
              </div>
            </div>
          </div>
        </div>
      </header>
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-8">
            <div class="container-fluid stats">
              <div class="row mb-3"><%- include('partials/actions.ejs') %></div>
              <div class="row stats-cards">
                <div class="col">
                  <div class="card">
                    <div class="card-body">
                      <div class="card-title">Total</div>
                      <h4 class="count-total">3434434434</h4>
                    </div>
                  </div>
                </div>
                <div class="col">
                  <div class="card">
                    <div class="card-body">
                      <div class="card-title">Recovered</div>
                      <h4 class="count-recovered">3434434434</h4>
                    </div>
                  </div>
                </div>
                <div class="col">
                  <div class="card">
                    <div class="card-body">
                      <div class="card-title">Deaths</div>
                      <h4 class="count-deaths">3434434434</h4>
                    </div>
                  </div>
                </div>
                <div class="col">
                  <div class="card">
                    <div class="card-body">
                      <div class="card-title">Active</div>
                      <h4 class="count-active">3434434434</h4>
                    </div>
                  </div>
                </div>
                <div class="col">
                  <div class="card">
                    <div class="card-body">
                      <div class="card-title">New</div>
                      <h4 class="count-deaths">3434434434</h4>
                    </div>
                  </div>
                </div>
                <div class="col">
                  <div class="card">
                    <div class="card-body">
                      <div class="card-title">Country</div>
                      <h4 class="count-country">3434434434</h4>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="container-fluid">
              <div class="col"><%- include('partials/country-list.ejs') %></div>
            </div>
          </div>
          <div class="col-md-4">
            <%- include('partials/right-content.ejs') %>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
